<?xml version="1.0" encoding="UTF-8" ?>
<configuration>
    <port>8089</port>
    <applicationName>jerry-rpc</applicationName>
    <compressType type = "gzip"/>
    <compressor class = "com.cj.jerry.rpc.compress.impl.GzipCompressor"/>
    <serializeType type = "jdk"/>
    <serializer class = "com.cj.jerry.rpc.serialize.impl.JdkSerializer"/>
    <registry url = "zookeeper://127.0.0.1:2181"/>
    <loadBalancer class = "com.cj.jerry.rpc.loadbalancer.impl.RoundRobinLoadBalancer"/>
<!--    <loadBalancer type = "minimumResponseTime"/>-->
    <idGenerator class = "com.cj.jerry.rpc.IdGenerator" dataCenterId = "1" machineId = "4"/>

</configuration>